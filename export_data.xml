<openerp>
	<data>
		<record id="iedp_export_data_wizard" model="ir.ui.view">
			<field name="name">IEDP Export Data Wizard</field>
			<field name="model">export.data.wizard</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<field name="state" invisible="True" />
				<group colspan="8" states="init">
					<field name="export_data_wizard_class_name" colspan="8" required="True" />
					<button special="cancel" string="Cancel" icon="gtk-cancel" />
					<button name="export_data" string="Compelete" type="object" icon="gtk-ok" />
				</group>
				<group states="done">
					<field name="export_data_wizard_data" filename="name" colspan="8" />
					<button special="cancel" string="Close" icon="gtk-ok" />
				</group>
			</field>
		</record>

		<record id="iedp_export_data_wizard_form_action" model="ir.actions.act_window">
			<field name="name">IEDP Export Data Wizard</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">export.data.wizard</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

		<menuitem id="iedp_export_data_wizard_menu" action="iedp_export_data_wizard_form_action"
			parent="base.menu_administration" />
	</data>
</openerp>